<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.notin.momotest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoMoTest">
        <activity android:name=".ui.view.LoginPasswordActivity"
            android:theme="@style/LoginStyle"
            ></activity>
        <activity
            android:name=".ui.view.AddInfoActivity"
            android:theme="@style/LoginStyle"></activity>
        <activity
            android:name=".ui.view.RegisterActivity"
            android:theme="@style/LoginStyle" />
        <activity
            android:name=".ui.view.AccuracyActivity"
            android:parentActivityName=".ui.view.LoginActivity"
            android:theme="@style/LoginStyle" />
        <activity
            android:name=".ui.view.LoginActivity"
            android:theme="@style/LoginStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.view.MainActivity"
            android:theme="@style/Theme.MainStyle">
        </activity>
    </application>

</manifest>